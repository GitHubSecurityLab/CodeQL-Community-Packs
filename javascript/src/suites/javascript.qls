- description: "GitHub's Community Packs JavaScript/TypeScript extended Suite"

- queries: '.'
  from: githubsecuritylab/codeql-javascript-queries

- include:
    kind:
    - problem
    - path-problem
    precision:
    - very-high
    - high
    - medium
    - low

# Remove debugging, and audit queries
- exclude:
    tags contain:
      - debugging
      - audit
      - deprecated

# Remove local testing folders
- exclude:
    query path:
      - /testing\/.*/
