- description: "GitHub's Community Packs CSharp Extended Suite"

- queries: '.'
  from: githubsecuritylab/codeql-csharp

- include:
    kind:
    - problem
    - path-problem
    - metric
    - diagnostic
    precision:
    - very-high
    - high
    - medium
    - low

# Remove debugging, and audit queries
- exclude:
    tags contain:
      - debugging
      - audit

# Remove local testing folders
- exclude:
    query path:
      - /testing\/.*/
